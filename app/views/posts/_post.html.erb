<div class="col-sm-12 well">
  <span class="content">
    <h4>
      <%= link_to post.title, post,  data: { turbolinks: 'false' }%>
    </h4>
   <span class="timestamp">
    Posted <%= time_ago_in_words(post.created_at) %> ago.
   </span>
   <hr />
    <p>
      <%= video_tag post.image_url.to_s, controls: true %>
    </p>
    <%= link_to 'Show', post, data: { turbolinks: 'false' }, class: "btn btn-default" %>
    <%= link_to 'Edit', edit_post_path(post), class: "btn btn-primary" %>
    <%= link_to 'Deleteee', post, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %>
  </span>

</div>