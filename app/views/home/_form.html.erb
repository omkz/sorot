<%= form_for(@video) do |f| %>
  <div class="form-group">
    <%= f.label 'title' %>
    <%= f.text_field :title, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label 'video' %>
    <%= f.file_field :image %>
  </div>
  <%= f.submit :post, class: 'btn btn-primary' %>
<% end %>
